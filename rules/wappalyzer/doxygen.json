{
    "name": "Doxygen",
    "matches": [
        {
            "regexp": "Doxygen ([\\d.]+)",
            "search": "meta[generator]",
            "offset": 1
        },
        {
            "regexp": "(?:<!-- Generated by Doxygen ([\\d.]+)|<link[^>]+doxygen\\.css)",
            "offset": 1
        }
    ],
    "website": "http://www.doxygen.nl/"
}