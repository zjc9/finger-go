{
    "name": "Sentry",
    "matches": [
        {
            "regexp": "<script[^>]*>\\s*Raven\\.config\\('[^']*', \\{\\s+release: '([0-9\\.]+)'",
            "offset": 1
        },
        {
            "regexp": "<script[^>]*src=\"[^\"]*browser\\.sentry\\-cdn\\.com/([0-9.]+)/bundle(?:\\.tracing)?(?:\\.min)?\\.js",
            "offset": 1
        }
    ],
    "website": "https://sentry.io/"
}