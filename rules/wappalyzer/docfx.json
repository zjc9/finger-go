{
    "name": "DocFX",
    "matches": [
        {
            "regexp": "toc.html",
            "search": "meta[docfx:tocrel]"
        },
        {
            "regexp": "docfx\\s([\\d\\.]+)",
            "search": "meta[generator]",
            "offset": 1
        },
        {
            "regexp": "toc.html",
            "search": "meta[docfx:navrel]"
        }
    ],
    "website": "https://github.com/dotnet/docfx"
}